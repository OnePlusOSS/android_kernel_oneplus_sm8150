	gpio-vaf = <0>;
        gpio-req-tbl-num = <0>;
        gpio-req-tbl-flags = <0>;
        gpio-req-tbl-label = "OIS_REAR_0";
        cci-device = <0>;
        cci-master = <1>;
        status = "ok";
    };

    ois_rear_1: qcom,ois@1 {
    };

    eeprom_rear_0: qcom,eeprom@7 {
        cell-index = <7>;
