};

&tlmm {
	display_panel_avdd_eldo_off: display_panel_avdd_eldo_off {
		mux {
			pins = "gpio130";
			function = "gpio";
		};
		config {
			pins = "gpio130";
			drive-strength = <8>;
			bias-disable = <0>;
			output-low;
		};
	};

	tri_state_key_active: tri_state_key_active {
		mux {
			pins = "gpio125", "gpio134", "gpio27";
			function = "gpio";
		};
		config {
			pins = "gpio125", "gpio134", "gpio27";
			drive-strength = <2>;
			bias-disable;
			};
		};

	tri_state_key_suspend: tri_state_key_suspend {
		mux {
			pins = "gpio125", "gpio134", "gpio27";
			function = "gpio";
		};
		config {
			pins = "gpio125", "gpio134", "gpio27";
			drive-strength = <2>;
			bias-disable;
			};
	};
};

