	};

	silead_fp {
		compatible = "sil,fingerprint";
		interrupt-parent = <&tlmm>;
		//avdd-supply = <&pm8150_l17>;
		avdd-gpios = <&tlmm 101 0x00>;
		irq-gpios = <&tlmm 118 0x00>;
		rst-gpios = <&tlmm 131 0x00>;
